<html>
<head>
    <title>Address</title>
</head>
<body>
<h1>Address</h1>
<table border="1">
    <tr><th>Address ID</th><th>Street</th><th>Zip Code</th></tr>
    <% for(var i=0; result.length > i; i++ ) { %>
    <tr>
        <td><a href="/address/?address_id=<%= result[i].address_id %>"><%= result[i].address_id%></a></td>
        <td><%= result[i].street %></td>
        <td><%= result[i].zip_code %></td>
    </tr>
    <% } %>
</table>
</body>

</html>